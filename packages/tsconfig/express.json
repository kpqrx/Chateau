{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Express",
  "extends": "./default.json",
  "compilerOptions": {
    "target": "es6",
    "lib": ["esnext"],
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true
  }
}
